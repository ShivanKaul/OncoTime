use vikramPatientData.grp
group Postalcode post = {A1B, C2D, D3E}


population is
    id: after 1000
    gender: male
    birthyear: before 1990
    postalcode: <post>
    diagnosis: brain

doctors is
    oncologist: true
    id: 1300 to 3100


{
    foreach Doctors d
    {
        foreach Patient p
        {
            foreach Diagnosis dg
            {
                print d
                print p
                print dg
            }
        }
    }
}
