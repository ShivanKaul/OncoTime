script DBTest()

/*
Simple test to get all patients from database and print each patient
*/

// ---- Filters ----
population is
    id: 1 to 999
    id : 1, 3
    id : before 5
    birthyear : 1959

doctors is
    oncologist: true 

// ---- Computations ----
{
    foreach Patient p {
        print p
    }
}
